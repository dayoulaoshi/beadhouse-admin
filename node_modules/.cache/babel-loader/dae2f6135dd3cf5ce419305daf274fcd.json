le\n      ref=\"singleTable\"\n      :data=\"tableData\"\n      highlight-current-row\n      @current-change=\"handleCurrentChange\"\n      style=\"width: 100%\"\n      :key=\"theKey\">\n      <el-table-column\n        type=\"index\"\n        width=\"50\">\n      </el-table-column>\n      <el-table-column\n        property=\"foodid\"\n        label=\"膳食信息id\"\n        width=\"120\">\n      </el-table-column>\n      <el-table-column\n        property=\"foodName\"\n        label=\"膳食名字\"\n        width=\"120\">\n      </el-table-column>\n      <el-table-column\n        property=\"customerName\"\n        label=\"供给顾客\">\n      </el-table-column>\n      <el-table-column\n        property=\"supplydate\"\n        label=\"供给日期\">\n      </el-table-column>\n      <el-table-column\n        property=\"supplyweekday\"\n        label=\"星期\">\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" min-width=\"300\">　　　\n        <template slot-scope=\"scope\">　　　　　　\n          <el-button type=\"primary\" @click=\"checkin_modify(scope.$index, scope.row)\">修改</el-button>\n          　　　　　　\n          <el-button type=\"primary\" @click=\"checkin_delete(scope.$index, scope.row)\">删除</el-button>\n          　　　　\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin-top: 20px;margin-left: 20px\">\n    </div>\n    <el-dialog title=\"修改\" :visible.sync=\"dialogFormVisible\">\n      <el-form :model=\"form\">\n        <el-form-item label=\"膳食id\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.foodManagementId\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"膳食名字\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.foodName\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"使用者id\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.customerid\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"使用者名字\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.customerName\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"使用日期\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.supplydate\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"使用礼拜\" :label-width=\"formLabelWidth\">\n          <el-input v-model=\"form.supplyweekday\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submit_form\">确 定</el-button>\n      </div>\n    </el-dialog>\n    <el-dialog title=\"删除\" :visible.sync=\"dialogdeleteFormVisible\">\n      <el-form>\n        <el-form-item label=\"确认删除？\" :label-width=\"formLabelWidth\">\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogdeleteFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"submit_delete\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n\n\n<script>\n  import {Message} from 'element-ui';\n\n  export default {\n    data() {\n      return {\n        tableData: [],\n        currentRow: null,\n        formLabelWidth: '100px',\n        form: {\n          foodManagementId:'',\n          foodid: 1,\n          customerid: 1,\n          supplydate: \"\",\n          supplyweekday: null,\n          foodName: \"\",\n          customerName: \"\"\n        },\n        delete_form: {\n          foodid: ''\n        },\n        dialogFormVisible: false,\n        dialogdeleteFormVisible: false,\n        theKey: 0\n      }\n    },\n    mounted() {\n      this.$axios.post('/FoodManagement/SelectAllWithNames')\n        .then((res) => {\n          let that = this\n          console.log(\"sussess\")\n          let datalist = res.data\n          if (datalist.status == 1) {\n            alert(\"错误码1\")\n          } else {\n            let len = datalist.length\n            console.log(len)\n            for (let i = 0; i < len; i++) {\n              that.tableData.push(datalist[i])\n            }\n            console.log(datalist[0])\n          }\n        })\n        .catch((err) => {\n          alert(\"获取数据失败\")\n          console.log(err)\n        })\n    },\n    methods: {\n      setCurrent(row) {\n        this.$refs.singleTable.setCurrentRow(row);\n      },\n      handleCurrentChange(val) {\n        this.currentRow = val;\n      },\n      checkin_modify(index, row) {\n        this.dialogFormVisible = true;\n        this.form = Object.assign({}, row);\n      },\n      checkin_delete(index, row) {\n        this.dialogdeleteFormVisible = true;\n        this.delete_form = Object.assign({}, row);\n      },\n      submit_form() {\n        let that = this\n        this.$axios.post('/FoodManagement/UpdateByPrimaryKey', {\n          username: this.form.customer_name,\n          bed_id: this.form.bed_id,\n          room_id: this.form.room_id\n        })\n          .then(function (response) {\n            console.log(response);\n            let data = response.data\n            if (data.code == 0) {\n              that.$message('修改成功');\n              this.theKey++;\n            } else {\n              that.$message('修改失败');\n              this.theKey++;\n            }\n          })\n          .catch(function (error) {\n            console.log(error);\n          });\n        this.dialogFormVisible = false;\n      },\n      submit_delete() {\n        let that = this\n        this.$axios.post('/FoodManagement/DeleteByPrimaryKey', {\n          foodid: this.delete_form.foodid\n        })\n          .then(function (response) {\n            console.log(response);\n            let data = response.data\n            if (data.code == 0) {\n              that.$message('删除成功');\n              this.theKey++;\n            } else {\n              that.$message('删除失败');\n              this.theKey++;\n            }\n          })\n          .catch(function (error) {\n            console.log(error);\n          });\n        this.dialogdeleteFormVisible = false;\n      }\n    }\n  }\n\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>\n"],"sourceRoot":"src/views/food_management"}]}